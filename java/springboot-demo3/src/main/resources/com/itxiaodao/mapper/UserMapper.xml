<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.itxiaodao.mapper.UserMapper">

    <update id="updateUser">
        UPDATE user
        SET nickname = #{nickname},
            email = #{email}
        WHERE id = #{id}
    </update>

</mapper>
