<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.itxiaodao.mapper.CategoryMapper">

    <insert id="addCategory" parameterType="com.itxiaodao.pojo.CategoryDTO">
        INSERT INTO category (name, alias)
        VALUES (#{name}, #{alias})
    </insert>



</mapper>
